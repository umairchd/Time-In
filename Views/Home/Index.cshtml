@model prj.Models.user
@{
    ViewBag.Title = "Index";
    Layout = "~/views/shared/_Layout.cshtml";
}


<br />
<br />
<br />

<div class="container-fluid">
    <!-- Carousel
    ================================================== -->
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img class="first-slide" src="~/Content/img/watches/37.jpg" alt="First slide">
                <div class="container">
                    
                </div>
            </div>
            <div class="item">
                <img class="second-slide" src="~/Content/img/watches/38.jpg" alt="Second slide">
                <div class="container">
                </div>
            </div>
            <div class="item">
                <img class="third-slide" src="~/Content/img/watches/36.jpg" alt="Third slide">
                <div class="container">
                </div>
            </div>
        </div>
        <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
            <span class="icon-prev" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
            <span class="icon-next" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    
    
        <br />
        <br />

    </div><!-- /.carousel -->

</div>